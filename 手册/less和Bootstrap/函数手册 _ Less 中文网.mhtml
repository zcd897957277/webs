an>) <span class=3D"hljs-keyword">when</span> (<span class=3D"hljs-function=
">iscolor</span>(<span class=3D"hljs-variable">@x</span>)) <span class=3D"h=
ljs-keyword">and</span> (<span class=3D"hljs-function">default</span>())  {=
<span class=3D"hljs-attribute">default-color</span>: <span class=3D"hljs-va=
riable">@x</span>}
  <span class=3D"hljs-class">.m</span>(<span class=3D"hljs-string">'foo'</s=
pan>)                                  {<span class=3D"hljs-attribute">case=
-1</span>: I am <span class=3D"hljs-string">'foo'</span>}
  <span class=3D"hljs-class">.m</span>(<span class=3D"hljs-string">'bar'</s=
pan>)                                  {<span class=3D"hljs-attribute">case=
-2</span>: I am <span class=3D"hljs-string">'bar'</span>}
