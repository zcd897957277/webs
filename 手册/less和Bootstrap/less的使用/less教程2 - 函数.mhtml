    {<span class=3D"hljs-attribute">case-1</span>: darkred}
  <span class=3D"hljs-class">.m</span>(blue)                               =
    {<span class=3D"hljs-attribute">case-2</span>: darkblue}
  <span class=3D"hljs-class">.m</span>(<span class=3D"hljs-variable">@x</sp=
an>) <span class=3D"hljs-keyword">when</span> (<span class=3D"hljs-function=
">iscolor</span>(<span class=3D"hljs-variable">@x</span>)) <span class=3D"h=
ljs-keyword">and</span> (<span class=3D"hljs-function">default</span>())  {=
<span class=3D"hljs-attribute">default-color</span>: <span class=3D"hljs-va=
riable">@x</span>}
  <span class=3D"hljs-class">.m</span>(<span class=3D"hljs-string">'foo'</s=
pan>)                                  {<span class=3D"hljs-attribute">case=
-1</span>: I am <span class=3D"hljs-string">'foo'</span>}
  <span class=3D"hljs-class">.m</span>(<span class=3D"hljs-string">'bar'</s=
pan>)                                  {<span class=3D"hljs-attribute">case=
-2</span>: I am <span class=3D"hljs-string">'bar'</span>}
  <span class=3D"hljs-class">.m</span>(<span class=3D"hljs-variable">@x</sp=
an>) <span class=3D"hljs-keyword">when</span> (<span class=3D"hljs-function=
">isstring</span>(<span class=3D"hljs-variable">@x</span>)) <span class=3D"=
hljs-keyword">and</span> (<span class=3D"hljs-function">default</span>()) {=
<span class=3D"hljs-attribute">default-string</span>: and I am the default}

  <span class=3D"hljs-keyword">&amp;</span><span class=3D"hljs-tag">-blue</=
span>  {<span class=3D"hljs-class">.m</span>(blue)}
  <span class=3D"hljs-keyword">&amp;</span><span class=3D"hljs-tag">-green<=
/span> {<span class=3D"hljs-class">.m</span>(green)}
  <span class=3D"hljs-keyword">&amp;</span><span class=3D"hljs-tag">-foo</s=
pan>   {<span class=3D"hljs-class">.m</span>(<span class=3D"hljs-string">'f=
oo'</span>)}
  <span class=3D"hljs-keyword">&amp;</span><span class=3D"hljs-tag">-baz</s=
pan>   {<span class=3D"hljs-class">.m</span>(<span class=3D"hljs-string">'b=
az'</span>)}
}
</code></pre>
<p>Result:</p>
<pre><code class=3D"lang-css"><span class=3D"hljs-class">.x-blue</span> {
  <span class=3D"hljs-attribute">case-2</span>: <span class=3D"hljs-hexcolo=
r">#00008b</span>;
}
<span class=3D"hljs-class">.x-green</span> {
